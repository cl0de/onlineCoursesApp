<% provide(:title, "New Course") %>
<h1>Create New Course</h1>

<div class="row">
    <div class="col-md-6 offset-md-3">
        <%= form_for (@course), html: { multipart: true } do |f| %>
            <%= render :partial => 'form', :locals =>{:f => f}  %>
            <%= f.submit "Create Course", class: "btn btn-primary" %>
        <% end %>
    </div>
</div>